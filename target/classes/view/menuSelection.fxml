<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="101.0" prefWidth="172.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stockManagementSystem.controller.MenuSelection">
   <children>
      <BorderPane prefHeight="100.0" prefWidth="176.0">
         <left>
            <VBox prefHeight="100.0" prefWidth="175.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="Create" mnemonicParsing="false" onAction="#goToCreate" prefHeight="50.0" prefWidth="225.0" text="Create Article">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="Search" mnemonicParsing="false" onAction="#goToSearch" prefHeight="50.0" prefWidth="225.0" text="Search Article">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
